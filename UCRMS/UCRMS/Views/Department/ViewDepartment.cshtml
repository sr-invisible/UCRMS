
@{
    ViewBag.Title = "ViewDepartment";
    Layout = "~/Views/Shared/_DepartmentView.cshtml";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Department
            <small>View All Departments</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
        </ol>
    </section>
    <!-------------------------------------------Common &  Menu Section---------------->
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <!-- right column -->
            <div class="col-sm-offset-3 col-md-6">
                <!-- Horizontal Form -->
                <div class="box box-info box-form">
                    <div class="box-header with-border">
                        <h3 class="box-title">View All Departments</h3>
                    </div><!-- /.box-header -->
                    <!-- form start -->
                    <form class="form-horizontal">
                        <div class="box-body">
                            <table id="myTable" class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th>Sl NO</th>
                                        <th>Code</th>
                                        <th>Name</th>

                                    </tr>
                                </thead>
                                <tfoot>
                                    <tr>
                                        <th>Id</th>
                                        <th>Code</th>
                                        <th>Name</th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div><!-- /.box-body -->
                        <div class="box-footer">
                        <button type="reset" id="Refresh" class="btn btn-info pull-right">Refresh</button>
                        </div><!-- /.box-footer -->
                    </form>
                </div><!-- /.box -->
            </div><!--/.col (right) -->
        </div>   <!-- /.row -->
    </section><!-- /.content -->
</div><!-- /.content-wrapper -->

